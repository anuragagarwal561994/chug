{
  "name": "chug",
  "description": "The caching build system",
  "keywords": [
    "cache",
    "caching",
    "build",
    "lightweight",
    "fast",
    "simple",
    "load",
    "build",
    "loader",
    "route",
    "router"
  ],
  "version": "0.2.0",
  "main": "chug.js",
  "homepage": "http://lighter.io/chug",
  "repository": "git://github.com/lighterio/chug.git",
  "author": "Sam Eubank <sameubank@gmail.com>",
  "contributors": [
    "Sam Eubank <sameubank@gmail.com>"
  ],
  "engines": [
    "node >= 0.2.6"
  ],
  "scripts": {
    "test": "./node_modules/exam/exam.js",
    "retest": "./node_modules/exam/exam.js --watch",
    "cover": "istanbul cover ./node_modules/exam/exam.js",
    "report": "open coverage/lcov-report/index.html",
    "coveralls": "istanbul cover ./node_modules/exam/exam.js --report lcovonly -- -R spec && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage"
  },
  "dependencies": {
    "uglify-js": "2.4.15",
    "csso": "1.3.11"
  },
  "devDependencies": {
    "clean-css": "2.2.11",
    "coffee-script": "1.7.1",
    "coveralls": "2.11.1",
    "exam": "0.0.5",
    "express": "4.7.2",
    "istanbul": "0.3.0",
    "less": "1.7.4",
    "ltl": "0.1.13",
    "markdown": "0.5.0",
    "node-sass": "0.9.3",
    "stylus": "0.47.3",
    "za": "0.0.18",
    "zeriousify": "0.1.9"
  },
  "bugs": {
    "url": "http://github.com/lighterio/chug/issues"
  },
  "licenses": [
