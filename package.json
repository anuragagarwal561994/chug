  },
  "devDependencies": {
    "assert-plus": "~0.1.5",
    "mocha": "~1.17.1",
    "istanbul": "~0.2.6",
    "express": "^3.4.8",
    "clean-css": "^2.1.6",
    "zeriousify": "^0.0.5"
  },
  "bugs": {
    "url": "http://github.com/zerious/chug/issues"
