  "keywords": [
    "cache",
    "caching",
    "build",
    "lightweight",
    "fast",
    "simple",
    "load",
    "build",
    "loader",
    "route",
    "router"
  ],
  "version": "0.1.1",
  "main": "chug.js",
  "homepage": "http://lighter.io/chug",
